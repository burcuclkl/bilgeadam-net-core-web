@{
    ViewData["Title"] = "Home Page";
}


<div class="row" style="margin-top: 50px;">

    <table class="table table-hover">
        <tr>
            <td>Id</td>
            <td>Restaurant Adı</td>
            <td>Restaurant'ın Adresi</td>
            <td>Restaurant'ın Menüsü</td>
            <td>Restaurant'ın Kuruluş Tarihi</td>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.RestaurantName</td>
                <td>@item.RestaurantAddress</td>
                <td>@item.RestaurantMenu</td>
                <td>@item.Date</td>
            </tr>
        }

    </table>

</div>
